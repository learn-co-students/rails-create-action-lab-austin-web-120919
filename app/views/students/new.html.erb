<!-- Create your form here -->
<h1>Student Form</h1>

<%= form_tag students_path do%>
    <label>Enter Student First Name: </label>
    <%= text_field_tag :first_name  %><br>


    <label>Enter Student Last Name: </label>
    <%= text_field_tag :last_name %><br>

    <input type='submit' id="Submit Student">

<% end %>
